  <div id='wrapper' >
    <!-- Sidebar -->
    <section id='sidebar'>
      <i class='icon-align-justify icon-large' id='toggle'></i>
      <ul id='dock'>
        <li class='launcher'>
          <i class='icon-dashboard'></i>
          <a href="dashboard.html">Dashboard</a>
        </li>
        <li class='active launcher'>
       <i class="icofont icofont-user-male"></i>
         <a><%= link_to 'Addmission',
          new_student_path %></a>
        </li>
        <li class='launcher'>
          <i class='icon-table'></i>
          <a ><%= link_to 'Tables',
            students_path %></a>
          </li>
          <li class='launcher dropdown hover'>
            <i class='icon-flag'></i>
            <a href='#'>Reports</a>
            <ul class='dropdown-menu'>
              <li class='dropdown-header'>Launcher description</li>
              <li>
                <a href='#'>Action</a>
              </li>
              <li>
                <a href='#'>Another action</a>
              </li>
              <li>
                <a href='#'>Something else here</a>
              </li>
            </ul>
          </li>
          <li class='launcher'>
            <i class='icon-bookmark'></i>
            <a href='#'>Bookmarks</a>
          </li>
          <li class='launcher'>
            <i class='icon-cloud'></i>
            <a href='#'>Backup</a>
          </li>
          <li class='launcher'>
            <i class='icon-bug'></i>
            <a href='#'>Feedback</a>
          </li>
        </ul>
        <div data-toggle='tooltip' id='beaker' title='Made by lab2023'></div>
      </section>
      <!-- Tools -->
      <section id='tools'>
        <ul class='breadcrumb' id='breadcrumb' style="background-color: #2c3e50; color: white">
          <li class='title'>Forms</li>
         </ul>
         <div id='toolbar'>

         </div>
       </section>
       <!-- Content -->
       <div id='content' style="background-color: #bdc3c7;">
        <div class='panel panel-default' style="background-color: #2c3e50; color: white">
          <div class='panel-heading' style="background-color: #2c3e50; color: white">
            <i class='icon-edit icon-large' ></i>
            Student
          </div>
          <div class='panel-body'  style="background-color: #2c3e50; color: white">
            <%= form_for @student, method: :put, url: wizard_path do |f| %>
         <fieldset>
         <div class="col-lg-6 left">
           <center>              
            <i class="icofont icofont-group-students " style="font-size: 100px;"></i><br>
            </center>
             <div class="form-group">
              <%= f.label :Father_Name %>
              <br />
              <%= f.text_field :father_name, rows: 10, :class => 'form-control' %>
                <h4> <%=f.error_span(:father_name) %></h4>
            </div>
            <div class="form-group">
              <%= f.label :Ocupasion %>
              <br />
              <%= f.text_field :ocupasion, :class => 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :Income %>
              <br />
              <%= f.text_field :income, :class => 'form-control' %>
            </div>            
            <div class="form-group">
              <%= f.label :Mother_Name%><br>
              <%= f.text_field :mother_name, :class => 'form-control' %>
            </div>
            
            <div class="form-group">
              <%= f.label :Address_Line1 %><br>
              <%= f.text_field :address_line1, :class => 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :Address_Line2 %><br>
              <%= f.text_field :address_line2, :class => 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :City%><br>
              <%= f.text_field :city, :class => 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :State%><br>
              <%= f.text_field :state, :class => 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :Pincode %><br>
              <%= f.text_field :pincode, :class => 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :Phone_Number%>
              <%= f.text_field :phone_no, :class => 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :Emergency_Number %><br>
              <%= f.text_field :emergency_no, :class => 'form-control' %>
            </div>

            <div class="form-group">
              <%= f.submit 'Submit', :class => 'btn btn-success both' %>
            </div>
            </div>
          </fieldset>
          <% end %>

        </div>
      </div>
      <div class='panel panel-default'>
        <div class='panel-heading'  style="background-color: #2c3e50; color: white">
          <i class='icon-edit icon-large'></i>
          <!--  Knob Inputs -->
        </div>
      </div>
    </div>
  </div>
  <!-- Footer -->
  <!-- Javascripts -->
