  <div id='wrapper'>
    <!-- Sidebar -->
    <section id='sidebar'>
      <i class='icon-align-justify icon-large' id='toggle'></i>
      <ul id='dock'>
        <li class='launcher'>
          <i class='icon-dashboard'></i>
          <a href="dashboard.html">Dashboard</a>
        </li>
        <li class='active launcher'>
          <i class="glyphicon glyphicon-user"></i>
          <a><%= link_to 'Addmission',
            new_student_path %></a>
          </li>
          <li class='launcher'>
            <i class='icon-table'></i>
            <a ><%= link_to 'Tables',
              students_path %></a>
            </li>
            <li class='launcher dropdown hover'>
              <i class='icon-flag'></i>
              <a href='#'>Reports</a>
              <ul class='dropdown-menu'>
                <li class='dropdown-header'>Launcher description</li>
                <li>
                  <a href='#'>Action</a>
                </li>
                <li>
                  <a href='#'>Another action</a>
                </li>
                <li>
                  <a href='#'>Something else here</a>
                </li>
              </ul>
            </li>
            <li class='launcher'>
              <i class='icon-bookmark'></i>
              <a href='#'>Bookmarks</a>
            </li>
            <li class='launcher'>
              <i class='icon-cloud'></i>
              <a href='#'>Backup</a>
            </li>
            <li class='launcher'>
              <i class='icon-bug'></i>
              <a href='#'>Feedback</a>
            </li>
          </ul>
          <div data-toggle='tooltip' id='beaker' title='Made by lab2023'></div>
        </section>
        <!-- Tools -->
        <section id='tools'>
          <ul class='breadcrumb' id='breadcrumb' style="background-color: #2c3e50; color: white">
            <li class='title'>Forms</li>
          </ul>
          <div id='toolbar'>

          </div>
        </section>
        <!-- Content -->
        <div id='content' style="background-color: #bdc3c7;">
          <div class='panel panel-default'>
            <div class='panel-heading' style="background-color: #2c3e50; color: white">
              <i class='icon-edit icon-large'></i>
              Student
            </div>
            <div class='panel-body' style="background-color: #2c3e50; color: white">
              
              <%= form_for @student, method: :put, url: wizard_path do |f| %>
              <% if f.object.errors.any? %>
              <div class="error_messages">
               <% f.object.errors.full_messages.each do |error| %>
               <p><%= error %></p>
               <% end %>
             </div>
             <% end %>

             <fieldset>
               

               <div class="form-group">
                <%= f.label :Addmission_Date, :class => 'control-label col-lg-2' %><br>
                <%= f.date_field :addmission_date, :class => 'form-control' %>
              </div>
              <div class="form-group">
                <%= f.label :first_name, :class => 'control-label col-lg-2' %><br>
                <%= f.text_field :first_name, :class => 'form-control' %>
              </div>

              <div class="form-group">
                <%= f.label :Last_name, :class => 'control-label col-lg-2' %><br>
                <%= f.text_field :last_name, :class => 'form-control' %>
              </div>
              <div class="form-group">
                <%= f.label :DoB, :class => 'control-label col-lg-2' %><br>
                <%= f.date_field :dob, :class => 'form-control' %>
              </div>

              <div class="form-group">
                <%= f.label :gender, :class => 'control-label col-lg-2' %><br>
                <%= f.select(:gender, [ 'Male', 'Female' ], {}, { :class => 'form-control' }) %>
              </div>
              <div class="form-group">
                <%= f.label :blood_group, :class => 'control-label col-lg-2' %><br>
                <%= f.select(:blood_group, ['A+', 'A-','B+', 'B-','AB+', 'AB-','O+', 'O-' ], {}, { :class => 'select form-control' }) %>
              </div>

              <div>
                <%= f.label :birth_place, :class => 'control-label col-lg-2' %><br>
                <%= f.text_field :birth_place, :class => 'form-control' %>
              </div>
              <div class="form-group">
                <%= f.label :category, :class => 'control-label col-lg-2' %><br>
                <%= f.text_field :category, :class => 'form-control' %>
              </div>
              <div class="form-group">
                <%= f.label :class, :class => 'control-label col-lg-2' %><br>
                <%= f.select(:student_class, ['8th','9th','10th', '11th','12th'], {}, { :class => 'select form-control' }) %>
              </div>
              <div class="form-group">
                <%= f.label :session, :class => 'control-label col-lg-2' %><br>
                 <%= f.select(:student_class, ['2012','2013', '2014','2015','2016','2017','2018'], {}, { :class => 'select form-control' }) %>
              </div>
              <!-- <h1>Contact</h1> -->
              <div class="form-group">
                <%= f.label :address_line1, :class => 'control-label col-lg-2' %><br>
                <%= f.text_field :address_line1, :class => 'form-control' %>
              </div>
              <div class="form-group">
               <%= f.label :address_line2, :class => 'control-label col-lg-2' %><br>
               <%= f.text_field :address_line2, :class => 'form-control' %>
             </div>
             <div class="form-group">
              <%= f.label :city, :class => 'control-label col-lg-2' %><br>
              <%= f.text_field :city, :class => 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :state, :class => 'control-label col-lg-2' %><br>
              <%= f.text_field :state, :class => 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :Pin_code, :class => 'control-label col-lg-2' %><br>
              <%= f.text_field :pincode, :class => 'form-control' %>
            </div>
            <div class="form-group">
              <%= f.label :Phone_no, :class => 'control-label col-lg-2' %><br>
              <%= f.text_field :phone_no, :class => 'form-control' %>
            </div>
            <h1>upload photo</h1>
            <div class="form-group">
             <%= f.label :photo, :class => 'control-label col-lg-2' %><br>
             <%= f.file_field :photo, :class => 'form-control' %>
           </div>
           <br>
           <div class="form-group">
             <%= f.submit 'Submit', :class => 'btn btn-success' %>
           </div>
         </fieldset>
         <% end %>

       </div>
     </div>
     <div class='panel panel-default' >
      <div class='panel-heading' style="background-color: #2c3e50; color: white">
        <i class='icon-edit icon-large'></i>
        <!--  Knob Inputs -->
      </div>
    </div>
  </div>
</div>
<!-- Footer -->
