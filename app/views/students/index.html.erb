      <td><%#= link_to student.id, student_path(student) %></td>
      <td><%#= student.student_id %></td>
      <td><%= student.name %></td>
      <td><%#= student.last_name %></td>
      <td><%= student.dob %></td>
      <td><%= student.gender %></td>
      <td><%#= student.blood_group %></td>
      <td><%#= student.birth_place %></td>
      <td><%#= student.category %></td>
      <td><%#= student.student_class %></td>
      <td><%#= student.session %></td>
      <td><%#= student.address_line1 %></td>
      <td><%#= student.address_line2 %></td>
      <td><%#= student.city %></td>
      <td><%#= student.state %></td>
      <td><%#= student.addmission_date %></td>
      <td><%#= student.father_name %></td>
      <td><%#= student.mother_name %></td>
      <td><%#= student.emergency_no %></td>
      <td><%#= student.previous_school_name %></td>
      <td><%#= student.obtained %></td>
      <td><%#= student.previous_class %></td>
      <td><%#= image_tag student.adharcard, :class =>'img-responsive' %></td>
      <td><%#= image_tag student.previous_school_marksheet, :class =>'img-responsive' %></td>
      <td><%#= student.pincode %></td>
      <td><%#= student.phone_no %></td>
      <td><%#= student.ocupasion %></td>
      <td><%#= student.income %></td>
      <td><%#=l student.created_at %></td>
       <td><%= link_to '', student_path(student), :class => "fa fa-eye color"%></td>
      <td>
      <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
        student_path(student),
        :method => :delete,
        :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
        :class => 'btn btn-xs btn-danger' %>
      </td>
    </tr>
    <% end %>
