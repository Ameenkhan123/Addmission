<div id='wrapper'>
  <!-- Sidebar -->
  <section id='sidebar'>
    <i class='icon-align-justify icon-large' id='toggle'></i>
    <ul id='dock'>
      <li class='launcher'>
        <i class='icon-dashboard'></i>
        <a href="dashboard.html">Dashboard</a>
      </li>
      <li class='active launcher'>
       <i class="icofont icofont-user-male"></i>
        <a><%= link_to 'Addmission',
          new_student_path %></a>
        </li>
        <li class='launcher'>
          <i class='icon-table'></i>
          <a ><%= link_to 'Tables',
            students_path %></a>
          </li>
          <li class='launcher dropdown hover'>
            <i class='icon-flag'></i>
            <a href='#'>Reports</a>
            <ul class='dropdown-menu'>
              <li class='dropdown-header'>Launcher description</li>
              <li>
                <a href='#'>Action</a>
              </li>
              <li>
                <a href='#'>Another action</a>
              </li>
              <li>
                <a href='#'>Something else here</a>
              </li>
            </ul>
          </li>
          <li class='launcher'>
            <i class='icon-bookmark'></i>
            <a href='#'>Bookmarks</a>
          </li>
          <li class='launcher'>
            <i class='icon-cloud'></i>
            <a href='#'>Backup</a>
          </li>
          <li class='launcher'>
            <i class='icon-bug'></i>
            <a href='#'>Feedback</a>
          </li>
        </ul>
        <div data-toggle='tooltip' id='beaker' title='Made by lab2023'></div>
      </section>
      <!-- Tools -->
      <section id='tools'>
        <ul class='breadcrumb' id='breadcrumb' style="background-color: #2c3e50">
          <li class='title'>Forms</li>
        </ul>
        <div id='toolbar' >

        </div>
      </section>
      <!-- Content -->
      <div id='content' style="background-color: #bdc3c7">
        <div class='panel panel-default' >
          <div class='panel-heading'  style="background-color:white ;">
            <i class='icon-edit icon-large'></i>
            Addmission
          </div>
          <div class='panel-body' style="background-color:#1c2a48;">
            <legend style="color: white ;">Student </legend>
            <div class="col-md-12" >
              <div class="col-md-2"  style="color: white ">
                <hr>
                <strong>Photo</strong><br>
                
                <%= image_tag @student.photo  , :class => 'img responsive size'  %>
                <hr></div>
                <div class="col-md-2"  style="color: white " >
                  <!-- <h1>Profile</h1> -->
                  <hr>
                  <strong>name:</strong><br>
                  <%= @student.name %>
                  <hr>
                  <div>
                    
                    <strong>Bod</strong><br>
                    <%= @student.dob %>
                  </div>
                  <hr>
                  <div>
                    <strong>Gender:</strong><br>
                    <%= @student.gender %>
                    
                  </div>
                  <hr>
                  <strong>father_name:</strong><br>
                  <%= @student.father_name %>
                  <hr>
                  <strong>mother_name:</strong><br>
                  <%= @student.mother_name %>
                  <hr>
                  <strong>blood_group:</strong><br>
                  <%= @student.blood_group %>
                  <hr>
                  <strong>addmission_date:</strong> 
                  <%=@student.addmission_date %>
                  <hr>
                </div>
                <div class="col-md-2" style="color: white ">
                  <!-- <h1>Content</h1> -->
                  <hr>
                  <strong>city:</strong><br>
                  <%= @student.city %>
                  <hr>
                  <div>
                    <strong>state:</strong><br>
                    <%= @student.state %>
                    
                  </div>
                  <hr>
                  <strong>pincode:</strong><br>
                  <%=  @student.pincode   %>
                  <hr>
                  <div>
                    
                    <strong>phone_no:</strong><br>
                    <%= @student.phone_no  %>
                  </div>
                  <hr>
                  <strong>address_line1:</strong><br>
                  <%= @student.address_line1 %>
                  <hr>
                  <strong>address_line2:</strong><br>
                  <%= @student.address_line2 %>

                </div>
                <div class="col-md-3" style="color: white ">
                  <hr>
                  <strong>:birth_place:</strong><br>
                  <%= @student.birth_place %>
                  <hr>
                  <strong>category: </strong><br>
                  <%= @student.category %>
                  <hr>
                  <strong>student_class:</strong><br>
                  <%= @student.student_class %>
                  <hr>
                  <strong>session:</strong><br>
                  <%= @student.session %>
                  <hr>
                  <strong>emergency_no:</strong><br>
                  <%= @student.emergency_no %>
                  <hr>
                  <strong>previous_school_name:</strong><br>
                  <%= @student.previous_school_name %>
                  <hr>
                  <strong>obtained:</strong><br>
                  <%= @student.obtained %>
                  <hr>
                  <strong>previous_class:</strong><br>
                  <%= @student.previous_class %>
                  <hr>
                  <strong>previous_school_marksheet:</strong><br>
                  <%= image_tag @student.previous_school_marksheet, :class =>'img responsive size' %>
                  <hr>
                  <strong>ocupasion:</strong><br>
                  <%= @student.ocupasion %>
                  <hr>
                  <strong>income:</strong><br>
                  <%= @student.income %>
                  <hr>
                </div>


                <div class="col-md-2" style="color: white ">
                  <hr>
                  <!--  <h1> Document</h1> -->
                  <strong>adharcard:</strong><br>
                  <%= image_tag @student.adharcard, :class => 'img responsive'  %>
                  <hr>

                </div>

              </div>

              <%= link_to t('.back', :default => t("helpers.links.back")),
              students_path, :class => 'btn btn-default'  %>
              <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              student_path(@student),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
            </div>
          </div>
          <div class='panel panel-default'>
            <div class='panel-heading' style="background-color: #2c3e50; color:white">
              <i class='icon-edit icon-large'></i>
              <!--  Knob Inputs -->
            </div>
          </div>
        </div>
      </div>
      <!-- Footer -->
      <!-- Javascripts -->
      <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js" type="text/javascript"></script><script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js" type="text/javascript"></script><script src="//cdnjs.cloudflare.com/ajax/libs/modernizr/2.6.2/modernizr.min.js" type="text/javascript"></script><script src="assets/javascripts/application-985b892b.js" type="text/javascript"></script>
