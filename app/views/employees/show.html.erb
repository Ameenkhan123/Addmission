          <hr>
          <strong>experience_certificate:</strong><br>
          <%= image_tag @employee.experience_certificate , :class => 'img responsive'   %>
          <hr>

     </div>

</div>

<%= link_to t('.back', :default => t("helpers.links.back")),
employees_path, :class => 'btn btn-default'  %>
