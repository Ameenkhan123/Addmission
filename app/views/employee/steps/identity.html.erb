
<div id='wrapper'>
	<!-- Sidebar -->
	<section id='sidebar'>
		<i class='icon-align-justify icon-large' id='toggle'></i>
		<ul id='dock'>
			<li class='launcher'>
				<i class='icon-dashboard'></i>
				<a href="dashboard.html">Dashboard</a>
			</li>
			<li class='active launcher'>
				<i class="icofont icofont-user-male"></i>
				<a><%= link_to 'Addmission',
					new_employee_path %></a>
				</li>
				<li class='launcher'>
					<i class='icon-table'></i>
					<a ><%= link_to 'Tables',
						employees_path %></a>
					</li>
					<li class='launcher dropdown hover'>
						<i class='icon-flag'></i>
						<a href='#'>Reports</a>
						<ul class='dropdown-menu'>
							<li class='dropdown-header'>Launcher description</li>
							<li>
								<a href='#'>Action</a>
							</li>
							<li>
								<a href='#'>Another action</a>
							</li>
							<li>
								<a href='#'>Something else here</a>
							</li>
						</ul>
					</li>
					<li class='launcher'>
						<i class='icon-bookmark'></i>
						<a href='#'>Bookmarks</a>
					</li>
					<li class='launcher'>
						<i class='icon-cloud'></i>
						<a href='#'>Backup</a>
					</li>
					<li class='launcher'>
						<i class='icon-bug'></i>
						<a href='#'>Feedback</a>
					</li>
				</ul>
				<div data-toggle='tooltip' id='beaker' title='Made by lab2023'></div>
			</section>
			<!-- Tools -->
			<section id='tools'>
				<ul class='breadcrumb' id='breadcrumb' style="background-color: #2c3e50; color: white">
					<li class='title'>Forms</li>
				</ul>
				<div id='toolbar'>

				</div>
			</section>
			<!-- Content -->
			<div id='content' style="background-color: #bdc3c7;">
				<div class='panel panel-default'>
					<div class='panel-heading' style="background-color: #2c3e50; color: white">
						<i class='icon-edit icon-large' ></i>
						Employee
					</div>
					<div class='panel-body' style="background-color: #2c3e50; color: white">
						<%= form_for @employee, method: :put, url: wizard_path do |f| %>
						<fieldset >
<div class="col-lg-6 left">
<center>

<i class="icofont icofont-architecture-alt"  style="font-size: 100px; "></i><br>
</center>
							<div class="form-group">
								<%= f.label :Joining_Date %><br>
								<%= f.date_field :joining_date, :class => 'form-control' %>
							</div>
							<div class="form-group">
								<%= f.label :First_Name %><br>
								<%= f.text_field :first_name, :class => 'form-control' %>
								  <p class='error'><%= show_errors(@employee, :first_name) %></p>
							</div>

							<div class="form-group">
								<%= f.label :Last_Name %><br>
								<%= f.text_field :last_name, :class => 'form-control' %>
							</div>
							<div class="form-group">
								<%= f.label :Dob %><br>
								<%= f.date_field :dob, :class => 'form-control' %>
							</div>

							<div class="form-group">
								<%= f.label :Gender %><br>
								<%= f.select(:gender, [ 'Male', 'Female' ], {}, { :class => 'select form-control' }) %>
							</div>

							<div class="form-group">
								<%= f.label :Department %><br>
								<%= f.select(:department, [ 'Computer Science', 'Information Technology'], {}, { :class => 'select form-control' }) %>
										  <p class='error'><%= show_errors(@employee, :department) %></p>
							</div>
							<div class="form-group">
								<%= f.label :Position%><br>
								<%= f.select(:position, [ 'Software developers', '  Software developers, applications','Web Developers','Computer and Information Systems Managers','Computer Hardware Engineers', 'Computer Network Architects','Computer Programmers', 'Computer Support Specialists

									', 'Computer Systems Analysts','Database Administrators'], {}, { :class => 'select form-control' }) %>
								</div>
								<!-- <h1>Contact</h1> -->
								<div class="form-group">
									<%= f.label :Job_Title %><br>
									<%= f.text_field :job_title, :class => 'form-control' %>
								</div><div class="form-group">
								<%= f.label :Qualification %><br>
								<%= f.text_field :qualification, :class => 'form-control' %>
							</div><div class="form-group">
							<%= f.label :Experience %><br>
							<%= f.text_field :experience, :class => 'form-control' %>
						</div>
						<div class="form-group">
							<%= f.label :Maritel_Status %><br>
							<%= f.select(:maritel_status, [ 'Yes', 'No'], {}, { :class => 'select form-control' }) %>
						</div>
						<div class="form-group">
							<%= f.label :Father_Name %><br>
							<%= f.text_field :father_name, :class => 'form-control' %>
						</div>
						<div class="form-group">
							<%= f.label :Mother_Name %><br>
							<%= f.text_field :mother_name, :class => 'form-control' %>
						</div>
						<div class="form-group">
							<%= f.label :Spocecom %><br>
							<%= f.text_field :spocecom, :class => 'form-control' %>
						</div>

						<div class="form-group">
							<%= f.label :Blood_Group %><br>
							<%= f.select(:blood_group, ['A+', 'A-','B+', 'B-','AB+', 'AB-','O+', 'O-' ], {}, { :class => 'select form-control' }) %>
						</div>
						<br>
						<h1>upload photo</h1>
						<div class="form-group">
							<%= f.label :Photo %>
							<br>
							<%= f.file_field :photo, :class => 'form-control' %>
						</div> <br>
						<div class="form-group">
							<%= f.submit 'Submit', :class => 'btn btn-success both' %>
						</div>
						</div>
					</fieldset>
					<% end %>
				</div>
			</div>
			<div class='panel panel-default'>
				<div class='panel-heading' style="background-color: #2c3e50; color: white">
					<i class='icon-edit icon-large'></i>
					<!--  Knob Inputs -->
				</div>
			</div>
		</div>
	</div>
	<!-- Footer -->
	<!-- Javascripts -->
	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js" type="text/javascript"></script><script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js" type="text/javascript"></script><script src="//cdnjs.cloudflare.com/ajax/libs/modernizr/2.6.2/modernizr.min.js" type="text/javascript"></script><script src="assets/javascripts/application-985b892b.js" type="text/javascript"></script>
